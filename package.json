{
  "name": "react-elastic-dnd",
  "version": "0.0.0-development",
  "description": "React Elastic dnd",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "test:watch": "mocha src/index.test.js -w @babel/register",
    "test:single": "mocha src/index.test.js",
    "semantic-release": "semantic-release",
    "start": "webpack serve --mode development --open --hot",
    "prebuild": "rimraf dist",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build": "npm-run-all --parallel build:*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Akbar1909/react-elastic-dnd.git"
  },
  "keywords": [
    "dnd",
    "react",
    "react-dnd",
    "elastic",
    "drag-and-drop",
    "drag",
    "drop"
  ],
  "files": [
    "dist/index.js",
    "dist/index.umd.js",
    "dist/index.umd.min.js",
    "README.md"
  ],
  "author": "Akbar, Muhammadsodiq <akbarbobomurodov19092000@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Akbar1909/react-elastic-dnd/issues"
  },
  "homepage": "https://github.com/Akbar1909/react-elastic-dnd#readme",
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "babel-loader": "9.1.2",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "7.0.0-beta.3",
    "chai": "4.3.7",
    "commitizen": "4.2.6",
    "css-loader": "6.7.3",
    "cz-conventional-changelog": "3.3.0",
    "ghooks": "2.0.4",
    "html-webpack-plugin": "5.5.0",
    "mocha": "10.2.0",
    "npm-run-all": "4.1.5",
    "semantic-release": "^20.0.0",
    "style-loader": "3.3.1",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single"
    }
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
